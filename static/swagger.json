    }
  },
  "paths" : {
    "/api/behavior/drive_off_charger" : {
      "post" : {
        "summary" : "Move the robot off of the charging station",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "Boolean true or false if successfully performed"
          },
          "503" : {
            "description" : "Unable to connect to robot"
          }
        }
      }
    },
    "/api/behavior/drive_on_charger" : {
      "post" : {
        "summary" : "Tell the robot to find and get onto the charging station",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "Boolean true or false if successfully performed"
          },
          "503" : {
            "description" : "Unable to connect to robot"
          }
        }
      }
    },
    "/api/behavior/dock_with_cube" : {
      "post" : {
        "summary" : "Tell the robot to dock with its cube",
        "produces" : [ "application/json" ],
        "responses" : {
          "200" : {
            "description" : "Boolean true or false if successfully performed"
          },
          "503" : {
            "description" : "Unable to connect to robot"
          }
        }
      }
    }
  },
  "schemes" : [ "http" ]
}